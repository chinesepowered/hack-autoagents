# EchoMind Environment Variables
# Copy to .env and fill in your API keys

# Database (provided by Render PostgreSQL in production)
DATABASE_URL=sqlite:///./echomind.db

# Reka Vision API — https://docs.reka.ai/
REKA_API_KEY=

# Modulate API — https://console.modulate.ai/developer.html
MODULATE_API_KEY=

# Fastino/GLiNER2 API — https://gliner.pioneer.ai/
FASTINO_API_KEY=

# Yutori API — https://docs.yutori.com/
YUTORI_API_KEY=

# Frontend URL (for CORS, set automatically on Render)
FRONTEND_URL=http://localhost:5173

# Environment
ENVIRONMENT=development
